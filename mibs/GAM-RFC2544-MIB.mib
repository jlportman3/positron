GAM-RFC2544-MIB DEFINITIONS ::= BEGIN

IMPORTS
    NOTIFICATION-GROUP, MODULE-COMPLIANCE, OBJECT-GROUP FROM SNMPv2-CONF
    NOTIFICATION-TYPE, MODULE-IDENTITY, OBJECT-TYPE FROM SNMPv2-SMI
    TEXTUAL-CONVENTION FROM SNMPv2-TC
    gamMgmt FROM POSITRON-SMI
    GAMDisplayString FROM GAM-TC
    GAMInterfaceIndex FROM GAM-TC
    GAMRowEditorState FROM GAM-TC
    GAMUnsigned8 FROM GAM-TC
    GAMVlanOrZero FROM GAM-TC
    Unsigned32 FROM SNMPv2-SMI
    MacAddress FROM SNMPv2-TC
    TruthValue FROM SNMPv2-TC
    ;

gamRfc2544Mib MODULE-IDENTITY
    LAST-UPDATED "201711270000Z"
    ORGANIZATION
        "Positron Access Solutions Corp."
    CONTACT-INFO
        "Customer Service <info@positronaccess.com>"
    DESCRIPTION
        "This is a private version of the RFC2544 MIB"
    REVISION    "201711270000Z"
    DESCRIPTION
        "Extend Rfc2544TestStatus"
    REVISION    "201510200000Z"
    DESCRIPTION
        "Initial version"
    ::= { gamMgmt 110 }


GAMRfc2544TestStatus ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "An integer that indicates the test status."
    SYNTAX      INTEGER { inactive(0), executing(1), cancelling(2),
                          cancelled(3), passed(4), failed(5),
                          failing(6) }

gamRfc2544MibObjects OBJECT IDENTIFIER
    ::= { gamRfc2544Mib 1 }

gamRfc2544Capabilities OBJECT IDENTIFIER
    ::= { gamRfc2544MibObjects 1 }

gamRfc2544CapabilitiesProfileCntMax OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Number of profiles that can be defined and saved to non-volatile
         memory."
    ::= { gamRfc2544Capabilities 1 }

gamRfc2544CapabilitiesReportCntMax OBJECT-TYPE
    SYNTAX      Unsigned32

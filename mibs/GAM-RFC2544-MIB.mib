gamRfc2544ConfigProfileTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF GAMRfc2544ConfigProfileEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is a table for managing RFC2544 profile entries."
    ::= { gamRfc2544ConfigProfile 1 }

gamRfc2544ConfigProfileEntry OBJECT-TYPE
    SYNTAX      GAMRfc2544ConfigProfileEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry has a set of parameters"
    INDEX       { gamRfc2544ConfigProfileName }
    ::= { gamRfc2544ConfigProfileTable 1 }

GAMRfc2544ConfigProfileEntry ::= SEQUENCE {
    gamRfc2544ConfigProfileName                GAMDisplayString,
    gamRfc2544ConfigProfileDescription         GAMDisplayString,
    gamRfc2544ConfigProfileDstOamAware         TruthValue,
    gamRfc2544ConfigProfileVlanId              GAMVlanOrZero,
    gamRfc2544ConfigProfileVlanPcp             Unsigned32,
    gamRfc2544ConfigProfileVlanDei             GAMUnsigned8,
    gamRfc2544ConfigProfileEgressInterface     GAMInterfaceIndex,
    gamRfc2544ConfigProfileMegLevel            Unsigned32,
    gamRfc2544ConfigProfileDmac                MacAddress,
    gamRfc2544ConfigProfileFrameSize64         TruthValue,
    gamRfc2544ConfigProfileFrameSize128        TruthValue,
    gamRfc2544ConfigProfileFrameSize256        TruthValue,
    gamRfc2544ConfigProfileFrameSize512        TruthValue,
    gamRfc2544ConfigProfileFrameSize1024       TruthValue,
    gamRfc2544ConfigProfileFrameSize1280       TruthValue,
    gamRfc2544ConfigProfileFrameSize1518       TruthValue,
    gamRfc2544ConfigProfileFrameSize2000       TruthValue,
    gamRfc2544ConfigProfileFrameSize9600       TruthValue,
    gamRfc2544ConfigProfileDwellTime           Unsigned32,
    gamRfc2544ConfigProfileSequenceCheck       TruthValue,
    gamRfc2544ConfigProfileTpEnable            TruthValue,
    gamRfc2544ConfigProfileTpTrialDuration     Unsigned32,
    gamRfc2544ConfigProfileTpRateMin           Unsigned32,
    gamRfc2544ConfigProfileTpRateMax           Unsigned32,
    gamRfc2544ConfigProfileTpRateStep          Unsigned32,
    gamRfc2544ConfigProfileTpAllowedFrameLoss  Unsigned32,
    gamRfc2544ConfigProfileLaEnable            TruthValue,
    gamRfc2544ConfigProfileLaTrialDuration     Unsigned32,
    gamRfc2544ConfigProfileLaDmInterval        Unsigned32,
    gamRfc2544ConfigProfileLaAllowedFrameLoss  Unsigned32,
    gamRfc2544ConfigProfileFlEnable            TruthValue,
    gamRfc2544ConfigProfileFlTrialDuration     Unsigned32,
    gamRfc2544ConfigProfileFlRateMin           Unsigned32,
    gamRfc2544ConfigProfileFlRateMax           Unsigned32,
    gamRfc2544ConfigProfileFlRateStep          Unsigned32,
    gamRfc2544ConfigProfileBbEnable            TruthValue,
    gamRfc2544ConfigProfileBbTrialDuration     Unsigned32,
    gamRfc2544ConfigProfileBbTrialCnt          Unsigned32,
    gamRfc2544ConfigProfileAction              GAMRowEditorState
}


        "Logical interface number."
    ::= { gamMstpConfigMstiInterfaceParamEntry 1 }

gamMstpConfigMstiInterfaceParamInstance OBJECT-TYPE
    SYNTAX      Integer32 (0..255)
    MAX-ACCESS  accessible-for-notify
    STATUS      current

-- *****************************************************************
-- MSTP-MIB:  Positron's GAM private MIB
-- *****************************************************************

GAM-MSTP-MIB DEFINITIONS ::= BEGIN

IMPORTS
    NOTIFICATION-GROUP, MODULE-COMPLIANCE, OBJECT-GROUP FROM SNMPv2-CONF
    NOTIFICATION-TYPE, MODULE-IDENTITY, OBJECT-TYPE FROM SNMPv2-SMI
    TEXTUAL-CONVENTION FROM SNMPv2-TC
    gamMgmt FROM POSITRON-SMI
    GAMDisplayString FROM GAM-TC
    GAMInterfaceIndex FROM GAM-TC
    GAMUnsigned16 FROM GAM-TC
    GAMUnsigned8 FROM GAM-TC
    GAMVlanListQuarter FROM GAM-TC
    Integer32 FROM SNMPv2-SMI
    Unsigned32 FROM SNMPv2-SMI
    TruthValue FROM SNMPv2-TC
    ;

gamMstpMib MODULE-IDENTITY
    LAST-UPDATED "201511130000Z"
    ORGANIZATION
        "Positron Access Solutions Corp."
    CONTACT-INFO
        "Customer Service <info@positronaccess.com>"
    DESCRIPTION
        "This is a private version of the 802.1Q-2005 MSTP MIB"
    REVISION    "201511130000Z"
    DESCRIPTION
        "Add TCN counters"
    REVISION    "201511040000Z"
    DESCRIPTION
        "Added msti config table with msti value as the iteration key"
    REVISION    "201509290000Z"
    DESCRIPTION
        "Added range information for bridge parameters"
    REVISION    "201407010000Z"
    DESCRIPTION
        "Initial version"
    ::= { gamMgmt 20 }


GAMMstpForceVersion ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration control the STP protocol variant to run."
    SYNTAX      INTEGER { stp(0), rstp(2), mstp(3) }

GAMMstpPoint2Point ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration describe the values of adminPointToPointMAC and
         operPointToPointMAC parameters. (Full duplex port administrative and
         operational status.) See 6.4.3 of IEEE Std 802.1D."
    SYNTAX      INTEGER { forceTrue(0), forceFalse(1), auto(2) }

GAMMstpPortState ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration describe the forwarding state of an interface."
    SYNTAX      INTEGER { disabled(0), discarding(1), learning(2),
                          forwarding(3) }

gamMstpMibObjects OBJECT IDENTIFIER
    ::= { gamMstpMib 1 }

gamMstpConfig OBJECT IDENTIFIER
    ::= { gamMstpMibObjects 2 }

gamMstpConfigBridgeParams OBJECT IDENTIFIER
    ::= { gamMstpConfig 1 }

gamMstpConfigBridgeParamsBridgeMaxAge OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Bridge Max Age, IEEE-802.1D-2004 sect 13.23.4"
    ::= { gamMstpConfigBridgeParams 1 }

gamMstpConfigBridgeParamsBridgeHelloTime OBJECT-TYPE
    SYNTAX      Unsigned32 (1..2)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Bridge Hello Time, 13.25.7 of IEEE-802.1Q-2005. Fixed value of two
         seconds by the standard, but this implementation allow a compatibility
         range from 1 to 2 seconds, in stipulated in 802.1Q-2005"
    ::= { gamMstpConfigBridgeParams 2 }

gamMstpConfigBridgeParamsBridgeForwardDelay OBJECT-TYPE
    SYNTAX      Unsigned32 (4..30)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Bridge Forward Delay, IEEE-802.1D-2004 sect 17.20"
    ::= { gamMstpConfigBridgeParams 3 }

gamMstpConfigBridgeParamsForceVersion OBJECT-TYPE
    SYNTAX      GAMMstpForceVersion
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "value of the Force Protocol Version parameter - 13.6.2 of
         IEEE-802.1Q-2005"
    ::= { gamMstpConfigBridgeParams 4 }

gamMstpConfigBridgeParamsTxHoldCount OBJECT-TYPE
    SYNTAX      Unsigned32 (1..10)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "TxHoldCount - 17.13.12 of IEEE Std 802.1D"
    ::= { gamMstpConfigBridgeParams 5 }

gamMstpConfigBridgeParamsMaxHops OBJECT-TYPE
    SYNTAX      GAMUnsigned8 (6..40)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "MaxHops - 13.22.1 of IEEE-802.1Q-2005"
    ::= { gamMstpConfigBridgeParams 6 }

gamMstpConfigBridgeParamsBpduFiltering OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "BPDU filtering for edge ports. Control whether a port explicitly
         configured as Edge will transmit and receive BPDUs"
    ::= { gamMstpConfigBridgeParams 7 }

gamMstpConfigBridgeParamsBpduGuard OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "BPDU guard for edge ports. Control whether a port explicitly configured
         as Edge will disable itself upon reception of a BPDU. The port will
         enter the error-disabled state, and will be removed from the active
         topology. "
    ::= { gamMstpConfigBridgeParams 8 }

gamMstpConfigBridgeParamsErrorRecoveryDelay OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The number of seconds until a STP inconsistent port is recovered. Valid
         values are zero (recovery disabled) or between 30 and 86400 (24 hours)"
    ::= { gamMstpConfigBridgeParams 9 }

gamMstpConfigMstiParamTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF GAMMstpConfigMstiParamEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is a table of the bridge instance (MSTIs) parameters"
    ::= { gamMstpConfig 2 }

gamMstpConfigMstiParamEntry OBJECT-TYPE
    SYNTAX      GAMMstpConfigMstiParamEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each MSTI has a set of parameters"
    INDEX       { gamMstpConfigMstiParamInstance }
    ::= { gamMstpConfigMstiParamTable 1 }

GAMMstpConfigMstiParamEntry ::= SEQUENCE {
    gamMstpConfigMstiParamInstance  Integer32,
    gamMstpConfigMstiParamPriority  GAMUnsigned8
}

gamMstpConfigMstiParamInstance OBJECT-TYPE
    SYNTAX      Integer32 (0..255)
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Bridge instance number. The CIST = 0, MSTI1 = 1, etc"
    ::= { gamMstpConfigMstiParamEntry 1 }

gamMstpConfigMstiParamPriority OBJECT-TYPE
    SYNTAX      GAMUnsigned8
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Bridge Priority"
    ::= { gamMstpConfigMstiParamEntry 2 }

gamMstpConfigMstiConfig OBJECT IDENTIFIER
    ::= { gamMstpConfig 3 }

gamMstpConfigMstiConfigId OBJECT IDENTIFIER
    ::= { gamMstpConfigMstiConfig 1 }

gamMstpConfigMstiConfigIdName OBJECT-TYPE
    SYNTAX      GAMDisplayString (SIZE(0..31))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The configuration name"
    ::= { gamMstpConfigMstiConfigId 1 }

gamMstpConfigMstiConfigIdRevision OBJECT-TYPE
    SYNTAX      GAMUnsigned16
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The configuration revision"
    ::= { gamMstpConfigMstiConfigId 2 }

gamMstpConfigMstiConfigTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF GAMMstpConfigMstiConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is the 802.1Q - 8.9 MST Configuration table
         
         For the purposes of calculating the Configuration Digest, the MST
         Configuration Table is considered to contain 4096 consecutive single
         octet elements, where each element of the table (with the exception of
         the first and last) contains an MSTID value."
    ::= { gamMstpConfigMstiConfig 2 }

gamMstpConfigMstiConfigEntry OBJECT-TYPE
    SYNTAX      GAMMstpConfigMstiConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The first element of the table contains the value 0, the second element
         the MSTID value corresponding to VID 1, the third element the MSTID
         value corresponding to VID 2, and so on, with the next to last element
         of the table containing the MSTID value corresponding to VID 4094, and
         the last element containing the value 0."
    INDEX       { gamMstpConfigMstiConfigVid }
    ::= { gamMstpConfigMstiConfigTable 1 }

GAMMstpConfigMstiConfigEntry ::= SEQUENCE {
    gamMstpConfigMstiConfigVid    Integer32,
    gamMstpConfigMstiConfigMstid  GAMUnsigned8
}

gamMstpConfigMstiConfigVid OBJECT-TYPE
    SYNTAX      Integer32 (1..4095)
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Vlan id"
    ::= { gamMstpConfigMstiConfigEntry 1 }

gamMstpConfigMstiConfigMstid OBJECT-TYPE
    SYNTAX      GAMUnsigned8
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The MSTID value associated with the vlan id"
    ::= { gamMstpConfigMstiConfigEntry 2 }

gamMstpConfigMstiConfigVlanBitmapTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF GAMMstpConfigMstiConfigVlanBitmapEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is the 802.1Q - 8.9 MST Configuration table
         
         For the purposes of calculating the Configuration Digest, the MST
         Configuration Table is considered to contain 8 consecutive single octet
         elements, where each element of the table contains an vlan bitmap
         (vlans that have the MSTI value specified are set to one)."
    ::= { gamMstpConfigMstiConfig 3 }

gamMstpConfigMstiConfigVlanBitmapEntry OBJECT-TYPE
    SYNTAX      GAMMstpConfigMstiConfigVlanBitmapEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The first element of the table contains vlan bitmap corresponding to
         MSTI 0, the second element contains vlan bitmap corresponding to MSTI 1
         until the 7th element. MSTI range is 0-7."
    INDEX       { gamMstpConfigMstiConfigVlanBitmapMstiValue }
    ::= { gamMstpConfigMstiConfigVlanBitmapTable 1 }

GAMMstpConfigMstiConfigVlanBitmapEntry ::= SEQUENCE {
    gamMstpConfigMstiConfigVlanBitmapMstiValue          Integer32,
    gamMstpConfigMstiConfigVlanBitmapAccessVlans0To1K   GAMVlanListQuarter,
    gamMstpConfigMstiConfigVlanBitmapAccessVlans1KTo2K  GAMVlanListQuarter,
    gamMstpConfigMstiConfigVlanBitmapAccessVlans2KTo3K  GAMVlanListQuarter,
    gamMstpConfigMstiConfigVlanBitmapAccessVlans3KTo4K  GAMVlanListQuarter
}

gamMstpConfigMstiConfigVlanBitmapMstiValue OBJECT-TYPE
    SYNTAX      Integer32 (0..7)
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "MSTI value."
    ::= { gamMstpConfigMstiConfigVlanBitmapEntry 1 }

gamMstpConfigMstiConfigVlanBitmapAccessVlans0To1K OBJECT-TYPE
    SYNTAX      GAMVlanListQuarter
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "First quarter of bit-array indicating the enabled access VLANs."
    ::= { gamMstpConfigMstiConfigVlanBitmapEntry 2 }

gamMstpConfigMstiConfigVlanBitmapAccessVlans1KTo2K OBJECT-TYPE
    SYNTAX      GAMVlanListQuarter
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Second quarter of bit-array indicating the enabled access VLANs."
    ::= { gamMstpConfigMstiConfigVlanBitmapEntry 3 }

gamMstpConfigMstiConfigVlanBitmapAccessVlans2KTo3K OBJECT-TYPE
    SYNTAX      GAMVlanListQuarter
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Third quarter of bit-array indicating the enabled access VLANs."
    ::= { gamMstpConfigMstiConfigVlanBitmapEntry 4 }

gamMstpConfigMstiConfigVlanBitmapAccessVlans3KTo4K OBJECT-TYPE
    SYNTAX      GAMVlanListQuarter
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Last quarter of bit-array indicating the enabled access VLANs."
    ::= { gamMstpConfigMstiConfigVlanBitmapEntry 5 }

gamMstpConfigCistInterfaceParamTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF GAMMstpConfigCistInterfaceParamEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is a table of the CIST physical interface parameters"
    ::= { gamMstpConfig 4 }

gamMstpConfigCistInterfaceParamEntry OBJECT-TYPE
    SYNTAX      GAMMstpConfigCistInterfaceParamEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each CIST physical interface has a set of parameters"
    INDEX       { gamMstpConfigCistInterfaceParamInterfaceNo }
    ::= { gamMstpConfigCistInterfaceParamTable 1 }

GAMMstpConfigCistInterfaceParamEntry ::= SEQUENCE {
    gamMstpConfigCistInterfaceParamInterfaceNo           GAMInterfaceIndex,
    gamMstpConfigCistInterfaceParamEnable                TruthValue,
    gamMstpConfigCistInterfaceParamAdminEdgePort         TruthValue,
    gamMstpConfigCistInterfaceParamAdminAutoEdgePort     TruthValue,
    gamMstpConfigCistInterfaceParamAdminPointToPointMAC  GAMMstpPoint2Point,
    gamMstpConfigCistInterfaceParamRestrictedRole        TruthValue,
    gamMstpConfigCistInterfaceParamRestrictedTcn         TruthValue,
    gamMstpConfigCistInterfaceParamBpduGuard             TruthValue
}

gamMstpConfigCistInterfaceParamInterfaceNo OBJECT-TYPE
    SYNTAX      GAMInterfaceIndex
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Logical interface number."
    ::= { gamMstpConfigCistInterfaceParamEntry 1 }

gamMstpConfigCistInterfaceParamEnable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Control whether port is controlled by xSTP. If disabled, the port
         forwarding state follow the MAC state"
    ::= { gamMstpConfigCistInterfaceParamEntry 2 }

gamMstpConfigCistInterfaceParamAdminEdgePort OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "adminEdgePort parameter - 18.3.3 of IEEE Std 802.1D"
    ::= { gamMstpConfigCistInterfaceParamEntry 3 }

gamMstpConfigCistInterfaceParamAdminAutoEdgePort OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "adminAutoEdgePort parameter - 17.13.3 of IEEE Std 802.1D"
    ::= { gamMstpConfigCistInterfaceParamEntry 4 }

gamMstpConfigCistInterfaceParamAdminPointToPointMAC OBJECT-TYPE
    SYNTAX      GAMMstpPoint2Point
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "adminPointToPointMAC parameter - 6.4.3 of IEEE Std 802.1D"
    ::= { gamMstpConfigCistInterfaceParamEntry 5 }

gamMstpConfigCistInterfaceParamRestrictedRole OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "restrictedRole parameter - 13.25.14 of IEEE Std 802.1Q-2005"
    ::= { gamMstpConfigCistInterfaceParamEntry 6 }

gamMstpConfigCistInterfaceParamRestrictedTcn OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "restrictedRole parameter - 13.25.15 of IEEE Std 802.1Q-2005"
    ::= { gamMstpConfigCistInterfaceParamEntry 7 }

gamMstpConfigCistInterfaceParamBpduGuard OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "If enabled, causes the port to disable itself upon receiving valid
         BPDU's. Contrary to the similar bridge setting, the port Edge status
         does not effect this setting. A port entering error-disabled state due
         to this setting is subject to the bridge ErrorRecoveryDelay setting as
         well"
    ::= { gamMstpConfigCistInterfaceParamEntry 8 }

gamMstpConfigAggrParams OBJECT IDENTIFIER
    ::= { gamMstpConfig 5 }

gamMstpConfigAggrParamsEnable OBJECT-TYPE

    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "priority field for the Port Identifier - 13.24.12 of 802.1Q-2005"
    ::= { gamMstpConfigMstiAggrParamEntry 4 }

gamMstpStatus OBJECT IDENTIFIER
    ::= { gamMstpMibObjects 3 }

gamMstpStatusBridgeTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF GAMMstpStatusBridgeEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table represent the status of the bridge instances"
    ::= { gamMstpStatus 1 }

gamMstpStatusBridgeEntry OBJECT-TYPE
    SYNTAX      GAMMstpStatusBridgeEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A MSTP Bridge instance set of status objects"
    INDEX       { gamMstpStatusBridgeInstance }
    ::= { gamMstpStatusBridgeTable 1 }

GAMMstpStatusBridgeEntry ::= SEQUENCE {
    gamMstpStatusBridgeInstance                 Integer32,
    gamMstpStatusBridgeBridgeId                 OCTET STRING,
    gamMstpStatusBridgeTimeSinceTopologyChange  Unsigned32,
    gamMstpStatusBridgeTopologyChangeCount      Unsigned32,
    gamMstpStatusBridgeTopologyChange           TruthValue,
    gamMstpStatusBridgeDesignatedRoot           OCTET STRING,
    gamMstpStatusBridgeRootPathCost             Unsigned32,
    gamMstpStatusBridgeRootPort                 Unsigned32,
    gamMstpStatusBridgeMaxAge                   Unsigned32,
    gamMstpStatusBridgeForwardDelay             Unsigned32,
    gamMstpStatusBridgeBridgeMaxAge             Unsigned32,
    gamMstpStatusBridgeBridgeHelloTime          Unsigned32,
    gamMstpStatusBridgeBridgeForwardDelay       Unsigned32,
    gamMstpStatusBridgeTxHoldCount              Unsigned32,
    gamMstpStatusBridgeForceVersion             GAMMstpForceVersion,
    gamMstpStatusBridgeCistRegionalRoot         OCTET STRING,
    gamMstpStatusBridgeCistInternalPathCost     Unsigned32,
    gamMstpStatusBridgeMaxHops                  GAMUnsigned8
}

gamMstpStatusBridgeInstance OBJECT-TYPE
    SYNTAX      Integer32 (0..255)
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Bridge instance number. The CIST = 0, MSTI1 = 1, etc"
    ::= { gamMstpStatusBridgeEntry 1 }

gamMstpStatusBridgeBridgeId OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(8))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Bridge Identifier of this bridge"
    ::= { gamMstpStatusBridgeEntry 2 }

gamMstpStatusBridgeTimeSinceTopologyChange OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The count in seconds of the time elapsed since the Topology Change flag
         was last True"
    ::= { gamMstpStatusBridgeEntry 3 }

gamMstpStatusBridgeTopologyChangeCount OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The count of the times the Topology Change flag parameter for the
         Bridge has been set since the Bridge was powered on or initialized"
    ::= { gamMstpStatusBridgeEntry 4 }

gamMstpStatusBridgeTopologyChange OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Topology Change Flag current status"
    ::= { gamMstpStatusBridgeEntry 5 }

gamMstpStatusBridgeDesignatedRoot OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(8))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Designated Root Bridge"
    ::= { gamMstpStatusBridgeEntry 6 }

gamMstpStatusBridgeRootPathCost OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Root Path Cost"

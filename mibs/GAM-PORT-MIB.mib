-- *****************************************************************
-- PORT-MIB:  Positron's GAM private MIB
-- *****************************************************************

GAM-PORT-MIB DEFINITIONS ::= BEGIN

IMPORTS
    NOTIFICATION-GROUP, MODULE-COMPLIANCE, OBJECT-GROUP FROM SNMPv2-CONF
    NOTIFICATION-TYPE, MODULE-IDENTITY, OBJECT-TYPE FROM SNMPv2-SMI
    TEXTUAL-CONVENTION FROM SNMPv2-TC
    gamMgmt FROM POSITRON-SMI
    GAMDisplayString FROM GAM-TC
    GAMInterfaceIndex FROM GAM-TC
    GAMPortStatusSpeed FROM GAM-TC
    GAMRfc5517Mode FROM GAM-TC
    GAMSfpTransceiver FROM GAM-TC
    GAMUnsigned8 FROM GAM-TC
    Counter64 FROM SNMPv2-SMI
    Integer32 FROM SNMPv2-SMI
    Unsigned32 FROM SNMPv2-SMI
    TruthValue FROM SNMPv2-TC
    ;

gamPortMib MODULE-IDENTITY
    LAST-UPDATED "201711270000Z"
    ORGANIZATION
        "Positron Access Solutions Corp."
    CONTACT-INFO
        "Customer Service <info@positronaccess.com>"
    DESCRIPTION
        "This is a private version of the PORT MIB"
    REVISION    "201711270000Z"
    DESCRIPTION
        "Changed PortConfigAdvertiseDisabled to Unsigned32"
    REVISION    "201701270000Z"
    DESCRIPTION
        "Added 802.3br statistics"
    REVISION    "201611280000Z"
    DESCRIPTION
        "Add 2.5G auto-negotiate capability"
    REVISION    "201507070000Z"
    DESCRIPTION
        "Port speed is moved into the TC MIB"
    REVISION    "201407010000Z"
    DESCRIPTION
        "Initial version"
    ::= { gamMgmt 11 }


GAMPortFc ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration controls the interface flow control."
    SYNTAX      INTEGER { off(0), on(1) }

GAMPortMedia ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration controls the interface media type."
    SYNTAX      INTEGER { rj45(0), sfp(1), dual(2) }

GAMPortSpeed ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration controls the interface speed. E.g force10ModeFdx means
         force 10Mbs full duplex."
    SYNTAX      INTEGER { force10ModeFdx(0), force10ModeHdx(1),
                          force100ModeFdx(2), force100ModeHdx(3),
                          force1GModeFdx(4), autoNegMode(5),
                          force2G5ModeFdx(6), force5GModeFdx(7),
                          force10GModeFdx(8), force12GModeFdx(9) }

gamPortMibObjects OBJECT IDENTIFIER
    ::= { gamPortMib 1 }

gamPortConfig OBJECT IDENTIFIER
    ::= { gamPortMibObjects 2 }

gamPortConfigTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF GAMPortConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is a table of the port interface parameters"
    ::= { gamPortConfig 1 }

gamPortConfigEntry OBJECT-TYPE
    SYNTAX      GAMPortConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each port interface has a set of parameters"
    INDEX       { gamPortConfigIfIndex }
    ::= { gamPortConfigTable 1 }

GAMPortConfigEntry ::= SEQUENCE {
    gamPortConfigIfIndex            GAMInterfaceIndex,
    gamPortConfigShutdown           TruthValue,
    gamPortConfigSpeed              GAMPortSpeed,
    gamPortConfigAdvertiseDisabled  Unsigned32,
    gamPortConfigMediaType          GAMPortMedia,
    gamPortConfigFC                 GAMPortFc,
    gamPortConfigMTU                Unsigned32,
    gamPortConfigExcessiveRestart   TruthValue,
    gamPortConfigPFC                GAMUnsigned8,
    gamPortConfigFrameLengthCheck   TruthValue,
    gamPortConfigRfc5517Mode        GAMRfc5517Mode
}

gamPortConfigIfIndex OBJECT-TYPE
    SYNTAX      GAMInterfaceIndex
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Logical interface number."
    ::= { gamPortConfigEntry 1 }

gamPortConfigShutdown OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Controls whether interface is shutdown or powered up. Set to TRUE in
         order to power down the interface."
    ::= { gamPortConfigEntry 2 }

gamPortConfigSpeed OBJECT-TYPE
    SYNTAX      GAMPortSpeed
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Controls the port speed and duplex."
    ::= { gamPortConfigEntry 3 }

gamPortConfigAdvertiseDisabled OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "In auto mode, bitmask that allows features not to be advertised.
         
         Bit 0: Disable half duplex advertising.
         
         Bit 1: Disable full duplex advertising.
         
         Bit 3: Disable 2.5G advertising.
         
         Bit 4: Disable 1G advertising.
         
         Bit 6: Disable 100M advertising.
         
         Bit 7: Disable 10M advertising.
         
         Bit 8: Disable 5G advertising.
         
         Bit 9: Disable 10G advertising.
         
         When not in auto mode, the value shall be zero."
    ::= { gamPortConfigEntry 4 }

gamPortConfigMediaType OBJECT-TYPE
    SYNTAX      GAMPortMedia
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Controls the port media type."
    ::= { gamPortConfigEntry 5 }

gamPortConfigFC OBJECT-TYPE
    SYNTAX      GAMPortFc
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Controls the port flow control mode."
    ::= { gamPortConfigEntry 6 }

gamPortConfigMTU OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Controls the port's Maximum Transmission Unit."
    ::= { gamPortConfigEntry 7 }

gamPortConfigExcessiveRestart OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "TRUE to restart half-duplex back-off algorithm after 16 collisions.
         FALSE to discard frame after 16 collisions"
    ::= { gamPortConfigEntry 8 }

gamPortConfigPFC OBJECT-TYPE
    SYNTAX      GAMUnsigned8
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "802.1Qbb Priority Flow Control bitmask, one bit for each prioritet.E.g.
         0x01 = prio 0, 0x80 = prio 7, 0xFF = prio 0-7"
    ::= { gamPortConfigEntry 9 }

gamPortConfigFrameLengthCheck OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "TRUE to enforce 802.3 frame length check (from Ethertype field). If
         enabled frames with length that doesn't match the frame length field
         will be dropped."
    ::= { gamPortConfigEntry 10 }

gamPortConfigRfc5517Mode OBJECT-TYPE
    SYNTAX      GAMRfc5517Mode
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Port RFC5517 mode. Applicable to 10G and MGMT ports only. Valid values
         are 'isolated', 'uplink', 'stacking' and 'stacking-isolated' on 10G
         port, and 'isolated' and 'uplink' on MGMT port."
    ::= { gamPortConfigEntry 11 }

gamPortStatus OBJECT IDENTIFIER
    ::= { gamPortMibObjects 3 }

gamPortStatusInformationTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF GAMPortStatusInformationEntry

    DESCRIPTION
        "Shows the current interface SFP type."
    ::= { gamPortStatusInformationEntry 6 }

gamPortStatusInformationSFPVendorName OBJECT-TYPE
    SYNTAX      GAMDisplayString (SIZE(0..19))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Shows the SFP vendor name."
    ::= { gamPortStatusInformationEntry 7 }

gamPortStatusInformationSFPVendorPN OBJECT-TYPE
    SYNTAX      GAMDisplayString (SIZE(0..19))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Shows the SFP vendor Product Number."
    ::= { gamPortStatusInformationEntry 8 }

gamPortStatusInformationSFPVendorRev OBJECT-TYPE
    SYNTAX      GAMDisplayString (SIZE(0..5))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Shows the SFP vendor Revision."
    ::= { gamPortStatusInformationEntry 9 }

gamPortStatusInformationSFPVendorSN OBJECT-TYPE
    SYNTAX      GAMDisplayString (SIZE(0..19))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Shows the SFP vendor Serial Number."
    ::= { gamPortStatusInformationEntry 10 }

gamPortStatusVeriPhyResult OBJECT IDENTIFIER
    ::= { gamPortStatus 3 }

gamPortControl OBJECT IDENTIFIER
    ::= { gamPortMibObjects 4 }

gamPortControlStatisticsClear OBJECT IDENTIFIER

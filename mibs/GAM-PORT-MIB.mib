    INDEX       { gamPortControlStatisticsClearIfIndex }
    ::= { gamPortControlStatisticsClearTable 1 }

GAMPortControlStatisticsClearEntry ::= SEQUENCE {
    gamPortControlStatisticsClearIfIndex          GAMInterfaceIndex,
    gamPortControlStatisticsClearStatisticsClear  TruthValue
}

gamPortControlStatisticsClearIfIndex OBJECT-TYPE
    SYNTAX      GAMInterfaceIndex
    MAX-ACCESS  accessible-for-notify
    STATUS      current

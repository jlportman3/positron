    gamPortStatisticsRmonStatisticsRx128to255Pkts     Counter64,
    gamPortStatisticsRmonStatisticsRx256to511Pkts     Counter64,
    gamPortStatisticsRmonStatisticsRx512to1023Pkts    Counter64,
    gamPortStatisticsRmonStatisticsRx1024to1518Pkts   Counter64,
    gamPortStatisticsRmonStatisticsRx1519PktsToMax    Counter64,
    gamPortStatisticsRmonStatisticsTxDropEvents       Counter64,

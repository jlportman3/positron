    gamPortStatusInformationFdx            TruthValue,
    gamPortStatusInformationFiber          TruthValue,
    gamPortStatusInformationSpeed          GAMPortStatusSpeed,
    gamPortStatusInformationSFPType        GAMSfpTransceiver,
    gamPortStatusInformationSFPVendorName  GAMDisplayString,
    gamPortStatusInformationSFPVendorPN    GAMDisplayString,
    gamPortStatusInformationSFPVendorRev   GAMDisplayString,
    gamPortStatusInformationSFPVendorSN    GAMDisplayString
}

gamPortStatusInformationIfIndex OBJECT-TYPE
    SYNTAX      GAMInterfaceIndex

    ::= { gamErpsStatusEntry 16 }

gamErpsStatusPort0RxRplBlocked OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the most recently received RPL Blocked flag on ring Port 0.
         Only relevant when Rx is active."
    ::= { gamErpsStatusEntry 17 }

gamErpsStatusPort0RxDoNotFlush OBJECT-TYPE

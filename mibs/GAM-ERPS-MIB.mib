gamErpsStatus OBJECT IDENTIFIER
    ::= { gamErpsMibObjects 3 }

gamErpsStatusTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF GAMErpsStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table contains status per EPRS group."
    ::= { gamErpsStatus 1 }

gamErpsStatusEntry OBJECT-TYPE
    SYNTAX      GAMErpsStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Status."
    INDEX       { gamErpsStatusGroupIndex }
    ::= { gamErpsStatusTable 1 }

GAMErpsStatusEntry ::= SEQUENCE {
    gamErpsStatusGroupIndex                   Integer32,
    gamErpsStatusActive                       TruthValue,
    gamErpsStatusProtectionState              GAMErpsProtectionState,
    gamErpsStatusRplBlocked                   TruthValue,
    gamErpsStatusWtrRemaining                 Unsigned32,
    gamErpsStatusAdminCmd                     GAMErpsAdminCmd,
    gamErpsStatusFopAlarm                     TruthValue,
    gamErpsStatusTxActive                     TruthValue,
    gamErpsStatusTxRequestOrState             GAMErpsRequestState,
    gamErpsStatusTxRplBlocked                 TruthValue,
    gamErpsStatusTxDoNotFlush                 TruthValue,
    gamErpsStatusBlockedPortReference         GAMErpsPort,
    gamErpsStatusPort0Blocked                 TruthValue,
    gamErpsStatusPort0State                   GAMErpsPortState,
    gamErpsStatusPort0RxActive                TruthValue,
    gamErpsStatusPort0RxRequestOrState        GAMErpsRequestState,
    gamErpsStatusPort0RxRplBlocked            TruthValue,
    gamErpsStatusPort0RxDoNotFlush            TruthValue,
    gamErpsStatusPort0RxBlockedPortReference  GAMErpsPort,
    gamErpsStatusPort0RxNodeId                OCTET STRING,
    gamErpsStatusPort1Blocked                 TruthValue,
    gamErpsStatusPort1State                   GAMErpsPortState,
    gamErpsStatusPort1RxActive                TruthValue,
    gamErpsStatusPort1RxRequestOrState        GAMErpsRequestState,
    gamErpsStatusPort1RxRplBlocked            TruthValue,
    gamErpsStatusPort1RxDoNotFlush            TruthValue,
    gamErpsStatusPort1RxBlockedPortReference  GAMErpsPort,
    gamErpsStatusPort1RxNodeId                OCTET STRING
}

gamErpsStatusGroupIndex OBJECT-TYPE
    SYNTAX      Integer32 (0..2147483647)
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "ERPS group index number. Valid range is (1..max groups). The maximum
         group number is platform-specific and can be retrieved from the ERPS
         capabilities."
    ::= { gamErpsStatusEntry 1 }

gamErpsStatusActive OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies whether ERPS group is currently active or not."
    ::= { gamErpsStatusEntry 2 }

gamErpsStatusProtectionState OBJECT-TYPE
    SYNTAX      GAMErpsProtectionState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies current ERPS group protection state."
    ::= { gamErpsStatusEntry 3 }

gamErpsStatusRplBlocked OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies whether the RPL is currently blocked or not."
    ::= { gamErpsStatusEntry 4 }

gamErpsStatusWtrRemaining OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the remaining wait-time before restoring ring."
    ::= { gamErpsStatusEntry 5 }

gamErpsStatusAdminCmd OBJECT-TYPE
    SYNTAX      GAMErpsAdminCmd
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the currently active administrative command."
    ::= { gamErpsStatusEntry 6 }

gamErpsStatusFopAlarm OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies whether the FailureOfProtocol alarm is currently active or
         not."
    ::= { gamErpsStatusEntry 7 }

gamErpsStatusTxActive OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies whether R-APS transmission is currently active."
    ::= { gamErpsStatusEntry 8 }

gamErpsStatusTxRequestOrState OBJECT-TYPE
    SYNTAX      GAMErpsRequestState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the currently transmitted request/state. Only relevant when
         Tx is active."
    ::= { gamErpsStatusEntry 9 }

gamErpsStatusTxRplBlocked OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the currently transmitted RPL Blocked flag. Only relevant
         when Tx is active."
    ::= { gamErpsStatusEntry 10 }

gamErpsStatusTxDoNotFlush OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the currently transmitted Do Not Flush flag. Only relevant
         when Tx is active."
    ::= { gamErpsStatusEntry 11 }

gamErpsStatusBlockedPortReference OBJECT-TYPE
    SYNTAX      GAMErpsPort
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the currently transmitted Blocked Port Reference. Only
         relevant when Tx is active."
    ::= { gamErpsStatusEntry 12 }

gamErpsStatusPort0Blocked OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies whether ring Port 0 is blocked or not."

-- *****************************************************************
-- ERPS-MIB:  Positron's GAM private MIB
-- *****************************************************************

GAM-ERPS-MIB DEFINITIONS ::= BEGIN

IMPORTS
    NOTIFICATION-GROUP, MODULE-COMPLIANCE, OBJECT-GROUP FROM SNMPv2-CONF
    NOTIFICATION-TYPE, MODULE-IDENTITY, OBJECT-TYPE FROM SNMPv2-SMI
    TEXTUAL-CONVENTION FROM SNMPv2-TC
    gamMgmt FROM POSITRON-SMI
    GAMInterfaceIndex FROM GAM-TC
    GAMRowEditorState FROM GAM-TC
    GAMVlanListQuarter FROM GAM-TC
    Counter64 FROM SNMPv2-SMI
    Integer32 FROM SNMPv2-SMI
    Unsigned32 FROM SNMPv2-SMI
    TruthValue FROM SNMPv2-TC
    ;

gamErpsMib MODULE-IDENTITY
    LAST-UPDATED "201407010000Z"
    ORGANIZATION
        "Positron Access Solutions Corp."
    CONTACT-INFO
        "Customer Service <info@positronaccess.com>"
    DESCRIPTION
        "Private MIB for Ethernet Ring Protection Switching, ERPS."
    REVISION    "201407010000Z"
    DESCRIPTION
        "Initial version"
    ::= { gamMgmt 72 }


GAMErpsAdminCmd ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies the Administrative command."
    SYNTAX      INTEGER { manualSwitch(1), forcedSwitch(2), clear(3) }

GAMErpsControlCmd ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies a control command."
    SYNTAX      INTEGER { none(0), admCmdForcedSwitch(1),
                          admCmdManualSwitch(2), admCmdClear(3),
                          statisticsClear(4) }

GAMErpsPort ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies a particular logical ring port."
    SYNTAX      INTEGER { port0(1), port1(2) }

GAMErpsPortState ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies the ring port state."
    SYNTAX      INTEGER { ok(1), signalFail(2) }

GAMErpsProtectionState ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies the ring protection state."
    SYNTAX      INTEGER { none(1), idle(2), protected(3),
                          forcedSwitch(4), manualSwitch(5),
                          pending(6) }

GAMErpsRequestState ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies a request/state."
    SYNTAX      INTEGER { none(1), manualSwitch(2), signalFail(3),
                          forcedSwitch(4), event(5) }

GAMErpsRingType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies the ERPS ring type."
    SYNTAX      INTEGER { major(1), sub(2) }

GAMErpsRplMode ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies the Ring Protection Link mode. Use 'none' if port is neither
         RPL owner nor neighbour."
    SYNTAX      INTEGER { none(1), owner(2), neighbour(3) }

GAMErpsVersion ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies the ERPS protocol version."
    SYNTAX      INTEGER { version1(1), version2(2) }

gamErpsMibObjects OBJECT IDENTIFIER
    ::= { gamErpsMib 1 }

gamErpsCapabilities OBJECT IDENTIFIER
    ::= { gamErpsMibObjects 1 }

gamErpsCapabilitiesMaxGroups OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Maximum number of configured ERPS groups."
    ::= { gamErpsCapabilities 1 }

gamErpsCapabilitiesMaxVlansPerGroup OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Maximum number of protected VLANS per ERPS group."
    ::= { gamErpsCapabilities 2 }

gamErpsConfig OBJECT IDENTIFIER
    ::= { gamErpsMibObjects 2 }

gamErpsConfigTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF GAMErpsConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is the ERPS group configuration table."
    ::= { gamErpsConfig 1 }

gamErpsConfigEntry OBJECT-TYPE
    SYNTAX      GAMErpsConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry in this table represents an ERPS group."
    INDEX       { gamErpsConfigGroupIndex }
    ::= { gamErpsConfigTable 1 }

GAMErpsConfigEntry ::= SEQUENCE {
    gamErpsConfigGroupIndex                       Integer32,
    gamErpsConfigRingType                         GAMErpsRingType,
    gamErpsConfigPort0                            GAMInterfaceIndex,
    gamErpsConfigPort1                            GAMInterfaceIndex,
    gamErpsConfigInterconnectMajorRingGroupIndex  Unsigned32,
    gamErpsConfigVirtualChannel                   TruthValue,
    gamErpsConfigPort0SignalFailMepIndex          Unsigned32,
    gamErpsConfigPort0ApsMepIndex                 Unsigned32,
    gamErpsConfigPort1SignalFailMepIndex          Unsigned32,
    gamErpsConfigPort1ApsMepIndex                 Unsigned32,
    gamErpsConfigHoldOffTime                      Unsigned32,
    gamErpsConfigWaitToRestoreTime                Unsigned32,
    gamErpsConfigGuardTime                        Unsigned32,
    gamErpsConfigRplMode                          GAMErpsRplMode,
    gamErpsConfigRplPort                          GAMErpsPort,
    gamErpsConfigRevertive                        TruthValue,
    gamErpsConfigVersion                          GAMErpsVersion,
    gamErpsConfigTopologyChange                   TruthValue,
    gamErpsConfigProtectedVlans0Kto1K             GAMVlanListQuarter,
    gamErpsConfigProtectedVlans1Kto2K             GAMVlanListQuarter,
    gamErpsConfigProtectedVlans2Kto3K             GAMVlanListQuarter,
    gamErpsConfigProtectedVlans3Kto4K             GAMVlanListQuarter,
    gamErpsConfigAction                           GAMRowEditorState
}

gamErpsConfigGroupIndex OBJECT-TYPE
    SYNTAX      Integer32 (0..2147483647)
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "ERPS group index number. Valid range is (1..max groups). The maximum
         group number is platform-specific and can be retrieved from the ERPS
         capabilities."
    ::= { gamErpsConfigEntry 1 }

gamErpsConfigRingType OBJECT-TYPE
    SYNTAX      GAMErpsRingType
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Type of ring. Can only be set once for a ring instance."
    ::= { gamErpsConfigEntry 2 }

gamErpsConfigPort0 OBJECT-TYPE
    SYNTAX      GAMInterfaceIndex
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "ifindex of ring protection port 0. Can only be set once for a ring
         instance."
    ::= { gamErpsConfigEntry 3 }

gamErpsConfigPort1 OBJECT-TYPE
    SYNTAX      GAMInterfaceIndex
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "ifindex of ring protection Port 1. For interconnected sub-rings this
         value must be zero. Can only be set once for a ring instance."
    ::= { gamErpsConfigEntry 4 }

gamErpsConfigInterconnectMajorRingGroupIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "For sub-ring: zero = not interconnected; > zero = index of major ring
         group. For major ring: zero = not interconnected; > zero = is
         interconnected (i.e flag-like semantics). Can only be set once for a
         ring instance."
    ::= { gamErpsConfigEntry 5 }

gamErpsConfigVirtualChannel OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Whether to use a virtual channel. Can only be set once for a ring
         instance."
    ::= { gamErpsConfigEntry 6 }

gamErpsConfigPort0SignalFailMepIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Index of SignalFail MEP for Port 0. Zero if not set. To clear an ERPS
         group's MEP association, set this to zero; it affects all MEP indices."
    ::= { gamErpsConfigEntry 7 }

gamErpsConfigPort0ApsMepIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Index of APS MEP for Port 0. Zero if not set."
    ::= { gamErpsConfigEntry 8 }

gamErpsConfigPort1SignalFailMepIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Index of SignalFail MEP for Port 1. Zero if not set. Must be zero for
         interconnected sub-rings."
    ::= { gamErpsConfigEntry 9 }

gamErpsConfigPort1ApsMepIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Index of APS MEP for Port 1. Zero if not set. Must be zero for
         interconnected sub-rings with virtual channel."
    ::= { gamErpsConfigEntry 10 }

gamErpsConfigHoldOffTime OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Hold off time in ms. Value is rounded down to 100ms precision. Valid
         range is 0-10000 ms"
    ::= { gamErpsConfigEntry 11 }

gamErpsConfigWaitToRestoreTime OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Wait-to-Restore time in ms. Valid range is 60000-720000 ms (1-12
         minutes)."
    ::= { gamErpsConfigEntry 12 }

gamErpsConfigGuardTime OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Guard time in ms. Valid range is 10-2000 ms."
    ::= { gamErpsConfigEntry 13 }

gamErpsConfigRplMode OBJECT-TYPE
    SYNTAX      GAMErpsRplMode
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Ring Protection Link mode."
    ::= { gamErpsConfigEntry 14 }

gamErpsConfigRplPort OBJECT-TYPE
    SYNTAX      GAMErpsPort
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "RPL port. Only valid if the ERPS group is RPL owner or neighbour."
    ::= { gamErpsConfigEntry 15 }

gamErpsConfigRevertive OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Revertive (true) or Non-revertive (false) mode."
    ::= { gamErpsConfigEntry 16 }

gamErpsConfigVersion OBJECT-TYPE
    SYNTAX      GAMErpsVersion
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "ERPS protocol version."
    ::= { gamErpsConfigEntry 17 }

gamErpsConfigTopologyChange OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Whether to enable Topology Change propagation; only valid for an
         interconnected node."
    ::= { gamErpsConfigEntry 18 }

gamErpsConfigProtectedVlans0Kto1K OBJECT-TYPE
    SYNTAX      GAMVlanListQuarter
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "First quarter of bit-array indicating whether a VLAN is protected by
         this ring instance ('1') or not ('0')."
    ::= { gamErpsConfigEntry 19 }

gamErpsConfigProtectedVlans1Kto2K OBJECT-TYPE
    SYNTAX      GAMVlanListQuarter
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Second quarter of bit-array indicating whether a VLAN is protected by
         this ring instance ('1') or not ('0')."
    ::= { gamErpsConfigEntry 20 }

gamErpsConfigProtectedVlans2Kto3K OBJECT-TYPE
    SYNTAX      GAMVlanListQuarter
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Third quarter of bit-array indicating whether a VLAN is protected by
         this ring instance ('1') or not ('0')."
    ::= { gamErpsConfigEntry 21 }

gamErpsConfigProtectedVlans3Kto4K OBJECT-TYPE
    SYNTAX      GAMVlanListQuarter
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Fourth quarter of bit-array indicating whether a VLAN is protected by
         this ring instance ('1') or not ('0')."
    ::= { gamErpsConfigEntry 22 }

gamErpsConfigAction OBJECT-TYPE
    SYNTAX      GAMRowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Action"
    ::= { gamErpsConfigEntry 100 }


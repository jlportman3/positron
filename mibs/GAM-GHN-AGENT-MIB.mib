    TruthValue FROM SNMPv2-TC
    ;

gamGhnAgentMib MODULE-IDENTITY
    LAST-UPDATED "201908270000Z"
    ORGANIZATION
        "Positron Access Solutions Corp."
    CONTACT-INFO
        "Customer Service <info@positronaccess.com>"
    DESCRIPTION
        "This is a private version of G.hn agent"
    REVISION    "201908270000Z"
    DESCRIPTION
        "Initial version"
    ::= { gamMgmt 143 }


GAMGhnAgentCPEType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the G.hn CPE types."
    SYNTAX      INTEGER { gamPortM(0), g1000m(1), g1000c(2),
                          g1001m(3), g1001c(4), gamCPEcoax(5),
                          gamCPEmimo(6), sfpMethod(7), g1001mr(8),
                          g1001cr(9), gamPortC(10), g1001mp(11),
                          g2002m(12), g1002c(13), g2002mPlus(14),
                          g1002cPlus(15), g1002m(16), g1002mPlus(17),
                          unknown(255) }

GAMGhnAgentEndpointState ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the G.hn endpoint state."
    SYNTAX      INTEGER { notDetected(0), ok(1),
                          quarantinedWrongPort(2),
                          quarantinedUnknownMAC(3) }

GAMGhnAgentFwUpgradeStage ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the G.hn device firmware upgrade stage."
    SYNTAX      INTEGER { initial(0), downloading(1), validating(2),
                          upgrading(3) }

GAMGhnAgentPoeInputSrcMode ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the PoE output status."
    SYNTAX      INTEGER { notFeedingPower(0), feedingPower(1), na(2) }

GAMGhnAgentPoeMode ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the PoE modes."
    SYNTAX      INTEGER { dot3af(0), dot3at(1), na(2) }

GAMGhnAgentPoeModeCtrl ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the PoE mode controls. 'Disable' will disable
         PoE. 'Enable' will enable PoE (if available). 'Force' enable PoE when
         the downstream equipment request 802.3at even if the endpoint source is
         802.3af."
    SYNTAX      INTEGER { disable(0), enable(1), force(2) }

GAMGhnAgentPortMode ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the G.hn port mode."
    SYNTAX      INTEGER { coax(0), mimo(1), siso(2), unknown(255) }

GAMGhnAgentPortRole ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the G.hn port role."
    SYNTAX      INTEGER { domainMaster(0), endpoint(1) }

GAMGhnAgentSubscriberModel ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the G.hn global subscriber model."
    SYNTAX      INTEGER { endpointAware(0), portAware(1) }

GAMGhnAgentVectorBoostState ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the G.hn VectorBoost(tm) state."
    SYNTAX      INTEGER { unavailable(0), measuring(1), ready(2) }


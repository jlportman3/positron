gamVlanConfigInterfacesSvlTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF GAMVlanConfigInterfacesSvlEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Shared VLAN Learning (SVL) allows for having one or more VLAN IDs map
         to the same Filter ID (FID). For a given set of VLANs, if an individual
         MAC address is learned in one VLAN, that learned information is used in
         forwarding decisions taken for that address relative to all other VLANs
         in the given set.
         
         fidCnt, which can be found in the capabilities section, indicates the
         number of FIDs available on this platform. The feature is not available
         if this number is 0."
    ::= { gamVlanConfigInterfaces 2 }

gamVlanConfigInterfacesSvlEntry OBJECT-TYPE
    SYNTAX      GAMVlanConfigInterfacesSvlEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The table is indexed by VLAN ID"
    INDEX       { gamVlanConfigInterfacesSvlVlanId }
    ::= { gamVlanConfigInterfacesSvlTable 1 }

GAMVlanConfigInterfacesSvlEntry ::= SEQUENCE {
    gamVlanConfigInterfacesSvlVlanId    GAMVlan,
    gamVlanConfigInterfacesSvlFilterId  GAMUnsigned16,
    gamVlanConfigInterfacesSvlAction    GAMRowEditorState
}

gamVlanConfigInterfacesSvlVlanId OBJECT-TYPE
    SYNTAX      GAMVlan
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "VLAN ID. Valid range is 1 - 4095."
    ::= { gamVlanConfigInterfacesSvlEntry 1 }

gamVlanConfigInterfacesSvlFilterId OBJECT-TYPE
    SYNTAX      GAMUnsigned16
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Filter ID (FID) used in Shared VLAN Learning. Zero or more VLANs may
         map into the same FID."
    ::= { gamVlanConfigInterfacesSvlEntry 2 }

gamVlanConfigInterfacesSvlAction OBJECT-TYPE
    SYNTAX      GAMRowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Action"
    ::= { gamVlanConfigInterfacesSvlEntry 100 }

gamVlanConfigInterfacesSvlTableRowEditor OBJECT IDENTIFIER

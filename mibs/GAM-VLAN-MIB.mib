    gamVlanConfigInterfacesTrunkVlans1KTo2K         GAMVlanListQuarter,
    gamVlanConfigInterfacesTrunkVlans2KTo3K         GAMVlanListQuarter,
    gamVlanConfigInterfacesTrunkVlans3KTo4K         GAMVlanListQuarter,
    gamVlanConfigInterfacesHybridNativeVlan         GAMVlan,
    gamVlanConfigInterfacesHybridPortType           GAMVlanPortType,
    gamVlanConfigInterfacesHybridIngressFiltering   TruthValue,
    gamVlanConfigInterfacesHybridIngressAcceptance  GAMVlanIngressAcceptance,
    gamVlanConfigInterfacesHybridEgressTagging      GAMVlanEgressTagging,
    gamVlanConfigInterfacesHybridVlans0KTo1K        GAMVlanListQuarter,
    gamVlanConfigInterfacesHybridVlans1KTo2K        GAMVlanListQuarter,
    gamVlanConfigInterfacesHybridVlans2KTo3K        GAMVlanListQuarter,
    gamVlanConfigInterfacesHybridVlans3KTo4K        GAMVlanListQuarter,

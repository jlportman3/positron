gamPerfMonitorConfigGlobalsTransfer OBJECT IDENTIFIER
    ::= { gamPerfMonitorConfigGlobals 2 }

gamPerfMonitorConfigGlobalsTransferAdminState OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Administrative control for PM's transfer function, TRUE is to enable
         sending PM's monitoring result and FALSE is to disable it."
    ::= { gamPerfMonitorConfigGlobalsTransfer 1 }

gamPerfMonitorConfigGlobalsTransferServerUrl OBJECT-TYPE
    SYNTAX      GAMDisplayString (SIZE(0..64))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The URL for the server directory that keeps PM's monitoring result."
    ::= { gamPerfMonitorConfigGlobalsTransfer 2 }

gamPerfMonitorConfigGlobalsTransferIntervalMode OBJECT-TYPE
    SYNTAX      GAMPerfMonitorTransferMode
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Transfer mode, based on selected interval type, for sending PM results.
         When 'all' is set, PM result from all available intervals will be sent.
         When 'new' is set, PM result from new intervals since last transfer
         will be sent. When 'fixAmount' is set, PM result from a fixed number of
         intervals will be sent."
    ::= { gamPerfMonitorConfigGlobalsTransfer 3 }

gamPerfMonitorConfigGlobalsTransferFixedNumberOfInterval OBJECT-TYPE
    SYNTAX      GAMUnsigned8 (1..96)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "PM result from this fixed number of intervals will be sent. It is
         working only when 'IntervalMode' is 'fixAmount'. The configurable value
         ranges from 1 to 96."
    ::= { gamPerfMonitorConfigGlobalsTransfer 4 }

gamPerfMonitorConfigGlobalsTransferRetryIncompletedTransfer OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Administrative control to retry sending PM result from last incompleted
         transmission, TRUE is to enable retrying the incompleted PM result
         transfer and FALSE is to disable it."
    ::= { gamPerfMonitorConfigGlobalsTransfer 5 }

gamPerfMonitorConfigGlobalsXferSched OBJECT IDENTIFIER

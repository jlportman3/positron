    ::= { gamIpStatisticsGlobalsIpv6 45 }

gamIpStatisticsInterfaces OBJECT IDENTIFIER
    ::= { gamIpStatistics 2 }

gamIpStatisticsInterfacesLinkTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF GAMIpStatisticsInterfacesLinkEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table provides interface link statistics for a given IP interface."
    ::= { gamIpStatisticsInterfaces 1 }

gamIpStatisticsInterfacesLinkEntry OBJECT-TYPE
    SYNTAX      GAMIpStatisticsInterfacesLinkEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry in this table represents an IP interface."
    INDEX       { gamIpStatisticsInterfacesLinkIfIndex }
    ::= { gamIpStatisticsInterfacesLinkTable 1 }

GAMIpStatisticsInterfacesLinkEntry ::= SEQUENCE {
    gamIpStatisticsInterfacesLinkIfIndex        GAMInterfaceIndex,
    gamIpStatisticsInterfacesLinkInPackets      Counter64,
    gamIpStatisticsInterfacesLinkOutPackets     Counter64,
    gamIpStatisticsInterfacesLinkInBytes        Counter64,
    gamIpStatisticsInterfacesLinkOutBytes       Counter64,
    gamIpStatisticsInterfacesLinkInMulticasts   Counter64,
    gamIpStatisticsInterfacesLinkOutMulticasts  Counter64,
    gamIpStatisticsInterfacesLinkInBroadcasts   Counter64,
    gamIpStatisticsInterfacesLinkOutBroadcasts  Counter64
}

gamIpStatisticsInterfacesLinkIfIndex OBJECT-TYPE
    SYNTAX      GAMInterfaceIndex
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Interface index number."
    ::= { gamIpStatisticsInterfacesLinkEntry 1 }

gamIpStatisticsInterfacesLinkInPackets OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Number of packets delivered by MAC layer to IP layer."
    ::= { gamIpStatisticsInterfacesLinkEntry 2 }

gamIpStatisticsInterfacesLinkOutPackets OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Number of packets IP protocols requested be transmitted, including
         those that were discarded or not sent."
    ::= { gamIpStatisticsInterfacesLinkEntry 3 }

gamIpStatisticsInterfacesLinkInBytes OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Number of octets received on the interface, including framing
         characters."
    ::= { gamIpStatisticsInterfacesLinkEntry 4 }

gamIpStatisticsInterfacesLinkOutBytes OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Number of octets transmitted out of the interface, including framing
         characters."
    ::= { gamIpStatisticsInterfacesLinkEntry 5 }

gamIpStatisticsInterfacesLinkInMulticasts OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Number of packets delivered by MAC layer to IP layer that were
         addressed to a MAC multicast address.
         
         For a MAC layer protocol, this includes both Group and Functional
         addresses."
    ::= { gamIpStatisticsInterfacesLinkEntry 6 }

gamIpStatisticsInterfacesLinkOutMulticasts OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Number of packets addressed to a multicast MAC address that IP
         protocols requested be transmitted, including those that were discarded
         or not sent.
         
         For a MAC layer protocol, this includes both Group and Functional
         addresses."
    ::= { gamIpStatisticsInterfacesLinkEntry 7 }

gamIpStatisticsInterfacesLinkInBroadcasts OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Number of packets delivered by MAC layer to IP layer that were
         addressed to a MAC broadcast address."
    ::= { gamIpStatisticsInterfacesLinkEntry 8 }

gamIpStatisticsInterfacesLinkOutBroadcasts OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Number of packets addressed to a broadcast MAC address that IP
         protocols requested be transmitted, including those that were discarded
         or not sent."
    ::= { gamIpStatisticsInterfacesLinkEntry 9 }

gamIpStatisticsInterfacesIpv4Table OBJECT-TYPE
    SYNTAX      SEQUENCE OF GAMIpStatisticsInterfacesIpv4Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table provides IPv4 related statitics for a given IP interface."
    ::= { gamIpStatisticsInterfaces 2 }


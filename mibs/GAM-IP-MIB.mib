    gamIpStatisticsInterfacesIpv6OutFragReqds        Unsigned32,
    gamIpStatisticsInterfacesIpv6OutFragOKs          Unsigned32,
    gamIpStatisticsInterfacesIpv6OutFragFails        Unsigned32,

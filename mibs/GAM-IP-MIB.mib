    gamIpStatisticsInterfacesIpv4OutRequests         Unsigned32,
    gamIpStatisticsInterfacesIpv4HCOutRequests       Counter64,
    gamIpStatisticsInterfacesIpv4OutNoRoutes         Unsigned32,
    gamIpStatisticsInterfacesIpv4OutForwDatagrams    Unsigned32,
    gamIpStatisticsInterfacesIpv4HCOutForwDatagrams  Counter64,
    gamIpStatisticsInterfacesIpv4OutDiscards         Unsigned32,
    gamIpStatisticsInterfacesIpv4OutFragReqds        Unsigned32,
    gamIpStatisticsInterfacesIpv4OutFragOKs          Unsigned32,
    gamIpStatisticsInterfacesIpv4OutFragFails        Unsigned32,
    gamIpStatisticsInterfacesIpv4OutFragCreates      Unsigned32,
    gamIpStatisticsInterfacesIpv4OutTransmits        Unsigned32,
    gamIpStatisticsInterfacesIpv4HCOutTransmits      Counter64,
    gamIpStatisticsInterfacesIpv4OutOctets           Unsigned32,
    gamIpStatisticsInterfacesIpv4HCOutOctets         Counter64,
    gamIpStatisticsInterfacesIpv4InMcastPkts         Unsigned32,
    gamIpStatisticsInterfacesIpv4HCInMcastPkts       Counter64,
    gamIpStatisticsInterfacesIpv4InMcastOctets       Unsigned32,
    gamIpStatisticsInterfacesIpv4HCInMcastOctets     Counter64,
    gamIpStatisticsInterfacesIpv4OutMcastPkts        Unsigned32,
    gamIpStatisticsInterfacesIpv4HCOutMcastPkts      Counter64,
    gamIpStatisticsInterfacesIpv4OutMcastOctets      Unsigned32,
    gamIpStatisticsInterfacesIpv4HCOutMcastOctets    Counter64,
    gamIpStatisticsInterfacesIpv4InBcastPkts         Unsigned32,

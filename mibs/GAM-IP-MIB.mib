gamIpConfigInterfacesIpv4Table OBJECT-TYPE
    SYNTAX      SEQUENCE OF GAMIpConfigInterfacesIpv4Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "IPv4 interface configuration table. This table enables IPv4 related
         configuration of the corresponding IP interface."
    ::= { gamIpConfigInterfaces 3 }

gamIpConfigInterfacesIpv4Entry OBJECT-TYPE
    SYNTAX      GAMIpConfigInterfacesIpv4Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry in this table represents an IP interface."
    INDEX       { gamIpConfigInterfacesIpv4IfIndex }
    ::= { gamIpConfigInterfacesIpv4Table 1 }

GAMIpConfigInterfacesIpv4Entry ::= SEQUENCE {
    gamIpConfigInterfacesIpv4IfIndex                    GAMInterfaceIndex,
    gamIpConfigInterfacesIpv4Active                     TruthValue,
    gamIpConfigInterfacesIpv4EnableDhcpClient           TruthValue,
    gamIpConfigInterfacesIpv4Ipv4Address                IpAddress,
    gamIpConfigInterfacesIpv4PrefixSize                 Unsigned32,
    gamIpConfigInterfacesIpv4DhcpClientFallbackTimeout  Unsigned32,
    gamIpConfigInterfacesIpv4DhcpClientHostname         GAMDisplayString,
    gamIpConfigInterfacesIpv4DhcpClientIdType           GAMIpDhcpClientType,
    gamIpConfigInterfacesIpv4DhcpClientIdIfMac          GAMInterfaceIndex,
    gamIpConfigInterfacesIpv4DhcpClientIdAscii          GAMDisplayString,
    gamIpConfigInterfacesIpv4DhcpClientIdHex            GAMDisplayString
}

gamIpConfigInterfacesIpv4IfIndex OBJECT-TYPE
    SYNTAX      GAMInterfaceIndex
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Interface index number."
    ::= { gamIpConfigInterfacesIpv4Entry 1 }

gamIpConfigInterfacesIpv4Active OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Enable IPv4.
         
         IPv4 can only be enabled if either the DHCP client is enabled, or a
         valid address has been configured."
    ::= { gamIpConfigInterfacesIpv4Entry 2 }

gamIpConfigInterfacesIpv4EnableDhcpClient OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Enable IPv4 DHCP client.
         
         Note: the DHCP client can only be enabled if there is no conflict in
         the values of: ipv4Address, prefixSize, and dhcpClientFallbackTimeout."
    ::= { gamIpConfigInterfacesIpv4Entry 3 }

gamIpConfigInterfacesIpv4Ipv4Address OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "IPv4 address.
         
         Note: Multiple interfaces may not have overlapping networks."
    ::= { gamIpConfigInterfacesIpv4Entry 4 }

gamIpConfigInterfacesIpv4PrefixSize OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Prefix size of the network.
         
         Note: Multiple interfaces may not have overlapping networks."
    ::= { gamIpConfigInterfacesIpv4Entry 5 }

gamIpConfigInterfacesIpv4DhcpClientFallbackTimeout OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "DHCP client fallback timer.
         
         If DHCP is disabled then this object has no effect. If DHCP is enabled
         and the fallback timeout value is different from zero, then this timer
         will stop the DHCP process and assign the ipv4Address to the interface
         instead."
    ::= { gamIpConfigInterfacesIpv4Entry 6 }

gamIpConfigInterfacesIpv4DhcpClientHostname OBJECT-TYPE
    SYNTAX      GAMDisplayString (SIZE(0..63))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The hostname of DHCP client. If DHCPv4 client is enabled, the
         configured hostname will be used in the DHCP option 12 field. When this
         value is empty string, the field use the configured system name plus
         the latest three bytes of system MAC addresses as the hostname."
    ::= { gamIpConfigInterfacesIpv4Entry 7 }

gamIpConfigInterfacesIpv4DhcpClientIdType OBJECT-TYPE
    SYNTAX      GAMIpDhcpClientType
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The type of the DHCP client identifier."
    ::= { gamIpConfigInterfacesIpv4Entry 8 }

gamIpConfigInterfacesIpv4DhcpClientIdIfMac OBJECT-TYPE
    SYNTAX      GAMInterfaceIndex
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The interface name of DHCP client identifier. When DHCPv4 client is
         enabled and the client identifier type is 'ifmac', the configured
         interface's hardware MAC address will be used in the DHCP option 61
         field."
    ::= { gamIpConfigInterfacesIpv4Entry 9 }

gamIpConfigInterfacesIpv4DhcpClientIdAscii OBJECT-TYPE
    SYNTAX      GAMDisplayString (SIZE(0..31))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The ASCII string of DHCP client identifier. When DHCPv4 client is
         enabled and the client identifier type is 'ascii', the ASCII string
         will be used in the DHCP option 61 field."
    ::= { gamIpConfigInterfacesIpv4Entry 10 }

gamIpConfigInterfacesIpv4DhcpClientIdHex OBJECT-TYPE
    SYNTAX      GAMDisplayString (SIZE(0..64))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The hexadecimal string of DHCP client identifier. When DHCPv4 client is
         enabled and the client identifier type 'hex', the hexadecimal value
         will be used in the DHCP option 61 field."
    ::= { gamIpConfigInterfacesIpv4Entry 11 }


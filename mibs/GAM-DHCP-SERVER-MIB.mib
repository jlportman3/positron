    DESCRIPTION
        "Action"
    ::= { gamDhcpServerConfigExcludedIpTableRowEditor 100 }

gamDhcpServerConfigPoolTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF GAMDhcpServerConfigPoolEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The table is DHCP server pool onfiguration table. The indexe is pool
         name."
    ::= { gamDhcpServerConfig 5 }

gamDhcpServerConfigPoolEntry OBJECT-TYPE
    SYNTAX      GAMDhcpServerConfigPoolEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry has a set of parameters"
    INDEX       { gamDhcpServerConfigPoolPoolName }
    ::= { gamDhcpServerConfigPoolTable 1 }

GAMDhcpServerConfigPoolEntry ::= SEQUENCE {
    gamDhcpServerConfigPoolPoolName               GAMDisplayString,
    gamDhcpServerConfigPoolPoolType               GAMDhcpServerPoolEnum,
    gamDhcpServerConfigPoolIpv4Address            IpAddress,
    gamDhcpServerConfigPoolSubnetMask             IpAddress,
    gamDhcpServerConfigPoolSubnetBroadcast        IpAddress,
    gamDhcpServerConfigPoolLeaseDay               Unsigned32,
    gamDhcpServerConfigPoolLeaseHour              Unsigned32,
    gamDhcpServerConfigPoolLeaseMinute            Unsigned32,
    gamDhcpServerConfigPoolDomainName             GAMDisplayString,
    gamDhcpServerConfigPoolDefaultRouter1         IpAddress,
    gamDhcpServerConfigPoolDefaultRouter2         IpAddress,
    gamDhcpServerConfigPoolDefaultRouter3         IpAddress,
    gamDhcpServerConfigPoolDefaultRouter4         IpAddress,
    gamDhcpServerConfigPoolDnsServer1             IpAddress,
    gamDhcpServerConfigPoolDnsServer2             IpAddress,
    gamDhcpServerConfigPoolDnsServer3             IpAddress,
    gamDhcpServerConfigPoolDnsServer4             IpAddress,
    gamDhcpServerConfigPoolNtpServer1             IpAddress,
    gamDhcpServerConfigPoolNtpServer2             IpAddress,
    gamDhcpServerConfigPoolNtpServer3             IpAddress,
    gamDhcpServerConfigPoolNtpServer4             IpAddress,
    gamDhcpServerConfigPoolNetbiosNodeType        GAMDhcpServerNetbiosNodeEnum,
    gamDhcpServerConfigPoolNetbiosScope           GAMDisplayString,
    gamDhcpServerConfigPoolNetbiosNameServer1     IpAddress,
    gamDhcpServerConfigPoolNetbiosNameServer2     IpAddress,
    gamDhcpServerConfigPoolNetbiosNameServer3     IpAddress,
    gamDhcpServerConfigPoolNetbiosNameServer4     IpAddress,
    gamDhcpServerConfigPoolNisDomainName          GAMDisplayString,
    gamDhcpServerConfigPoolNisServer1             IpAddress,
    gamDhcpServerConfigPoolNisServer2             IpAddress,
    gamDhcpServerConfigPoolNisServer3             IpAddress,
    gamDhcpServerConfigPoolNisServer4             IpAddress,
    gamDhcpServerConfigPoolClientIdentifierType   GAMDhcpServerClientIdentifierEnum,
    gamDhcpServerConfigPoolClientIdentifierName   GAMDisplayString,
    gamDhcpServerConfigPoolClientIdentifierMac    MacAddress,
    gamDhcpServerConfigPoolClientHardwareAddress  MacAddress,
    gamDhcpServerConfigPoolClientName             GAMDisplayString,
    gamDhcpServerConfigPoolVendorClassId1         GAMDisplayString,
    gamDhcpServerConfigPoolVendorSpecificInfo1    GAMDisplayString,
    gamDhcpServerConfigPoolVendorClassId2         GAMDisplayString,
    gamDhcpServerConfigPoolVendorSpecificInfo2    GAMDisplayString,
    gamDhcpServerConfigPoolVendorClassId3         GAMDisplayString,
    gamDhcpServerConfigPoolVendorSpecificInfo3    GAMDisplayString,
    gamDhcpServerConfigPoolVendorClassId4         GAMDisplayString,
    gamDhcpServerConfigPoolVendorSpecificInfo4    GAMDisplayString,
    gamDhcpServerConfigPoolAction                 GAMRowEditorState
}

gamDhcpServerConfigPoolPoolName OBJECT-TYPE
    SYNTAX      GAMDisplayString (SIZE(0..32))
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Name of DHCP pool."
    ::= { gamDhcpServerConfigPoolEntry 1 }

gamDhcpServerConfigPoolPoolType OBJECT-TYPE
    SYNTAX      GAMDhcpServerPoolEnum
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Type of pool. none(0) means the pool type is not defined yet.
         network(1) means the pool defines a pool of IP addresses to service
         more than one DHCP client. host(2) means the pool services for a
         specific DHCP client identified by client identifier or hardware
         address."
    ::= { gamDhcpServerConfigPoolEntry 2 }

gamDhcpServerConfigPoolIpv4Address OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Network number of the subnet. If the pool type is of network, the IP
         address can be any general IP address. If the pool type is of host, the
         IP address must be a unicast IP address."
    ::= { gamDhcpServerConfigPoolEntry 3 }

gamDhcpServerConfigPoolSubnetMask OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Subnet Mask. DHCP option 1. Specify subnet mask of the DHCP address
         pool, excluding 0.0.0.0 and 255.255.255.255."
    ::= { gamDhcpServerConfigPoolEntry 4 }

gamDhcpServerConfigPoolSubnetBroadcast OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Broadcast IP address in the subnet. DHCP option 28. Specify the
         broadcast address in use on the client's subnet."
    ::= { gamDhcpServerConfigPoolEntry 5 }

gamDhcpServerConfigPoolLeaseDay OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Number of days of lease time. DHCP option 51, 58 and 59. The value
         range is 0-365. Specify lease time that allows the client to request a
         lease time for the IP address. If all of LeaseDay, LeaseHour and
         LeaseMinute are 0's, then it means the lease time is infinite."
    ::= { gamDhcpServerConfigPoolEntry 6 }

gamDhcpServerConfigPoolLeaseHour OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Number of hours of lease time. DHCP option 51, 58 and 59. The value
         range is 0-23. Specify lease time that allows the client to request a
         lease time for the IP address. If all of LeaseDay, LeaseHour and
         LeaseMinute are 0's, then it means the lease time is infinite."
    ::= { gamDhcpServerConfigPoolEntry 7 }

gamDhcpServerConfigPoolLeaseMinute OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Number of minutes of lease time. DHCP option 51, 58 and 59. The value
         range is 0-59. Specify lease time that allows the client to request a
         lease time for the IP address. If all of LeaseDay, LeaseHour and
         LeaseMinute are 0's, then it means the lease time is infinite."
    ::= { gamDhcpServerConfigPoolEntry 8 }

gamDhcpServerConfigPoolDomainName OBJECT-TYPE
    SYNTAX      GAMDisplayString (SIZE(0..128))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Domain name. DHCP option 15. Specify domain name that client should use
         when resolving hostname via DNS."
    ::= { gamDhcpServerConfigPoolEntry 9 }

gamDhcpServerConfigPoolDefaultRouter1 OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Default router 1."
    ::= { gamDhcpServerConfigPoolEntry 10 }

gamDhcpServerConfigPoolDefaultRouter2 OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write

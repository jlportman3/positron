-- *****************************************************************
-- DHCP-SERVER-MIB:  Positron's GAM private MIB
-- *****************************************************************

GAM-DHCP-SERVER-MIB DEFINITIONS ::= BEGIN

IMPORTS
    NOTIFICATION-GROUP, MODULE-COMPLIANCE, OBJECT-GROUP FROM SNMPv2-CONF
    NOTIFICATION-TYPE, MODULE-IDENTITY, OBJECT-TYPE FROM SNMPv2-SMI
    TEXTUAL-CONVENTION FROM SNMPv2-TC
    gamMgmt FROM POSITRON-SMI
    GAMDisplayString FROM GAM-TC
    GAMInterfaceIndex FROM GAM-TC
    GAMRowEditorState FROM GAM-TC
    GAMUnsigned16 FROM GAM-TC
    Integer32 FROM SNMPv2-SMI
    IpAddress FROM SNMPv2-SMI
    Unsigned32 FROM SNMPv2-SMI
    MacAddress FROM SNMPv2-TC
    TruthValue FROM SNMPv2-TC
    ;

gamDhcpServerMib MODULE-IDENTITY
    LAST-UPDATED "201508240000Z"
    ORGANIZATION
        "Positron Access Solutions Corp."
    CONTACT-INFO
        "Customer Service <info@positronaccess.com>"
    DESCRIPTION
        "This is a private version of DhcpServer"
    REVISION    "201508240000Z"
    DESCRIPTION
        "revise fqdn as name"
    REVISION    "201411270000Z"
    DESCRIPTION
        "revise descriptions by Palle"
    REVISION    "201407010000Z"
    DESCRIPTION
        "Initial version"
    ::= { gamMgmt 109 }


GAMDhcpServerBindingEnum ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the type of binding."
    SYNTAX      INTEGER { none(0), automatic(1), manual(2),
                          expired(3) }

GAMDhcpServerBindingStateEnum ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the state of binding."
    SYNTAX      INTEGER { none(0), allocated(1), committed(2),
                          expired(3) }

GAMDhcpServerClientIdentifierEnum ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the type of client identifier."
    SYNTAX      INTEGER { none(0), name(1), mac(2) }

GAMDhcpServerNetbiosNodeEnum ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the type of NetBIOS node."
    SYNTAX      INTEGER { nodeNone(0), nodeB(1), nodeP(2), nodeM(3),
                          nodeH(4) }

GAMDhcpServerPoolEnum ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the type of DHCP pool."
    SYNTAX      INTEGER { none(0), network(1), host(2) }

gamDhcpServerMibObjects OBJECT IDENTIFIER
    ::= { gamDhcpServerMib 1 }

gamDhcpServerConfig OBJECT IDENTIFIER
    ::= { gamDhcpServerMibObjects 2 }

gamDhcpServerConfigGlobals OBJECT IDENTIFIER
    ::= { gamDhcpServerConfig 1 }

gamDhcpServerConfigGlobalsMode OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Global mode of DHCP server. true is to enable the functions of DHCP
         server and false is to disable it."
    ::= { gamDhcpServerConfigGlobals 1 }

gamDhcpServerConfigVlanTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF GAMDhcpServerConfigVlanEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is the table of DHCP server VLAN configuration. The index is VLAN
         ID."
    ::= { gamDhcpServerConfig 2 }

gamDhcpServerConfigVlanEntry OBJECT-TYPE
    SYNTAX      GAMDhcpServerConfigVlanEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each VLAN has a set of parameters"
    INDEX       { gamDhcpServerConfigVlanIfIndex }
    ::= { gamDhcpServerConfigVlanTable 1 }

GAMDhcpServerConfigVlanEntry ::= SEQUENCE {
    gamDhcpServerConfigVlanIfIndex  GAMInterfaceIndex,
    gamDhcpServerConfigVlanMode     TruthValue
}

gamDhcpServerConfigVlanIfIndex OBJECT-TYPE
    SYNTAX      GAMInterfaceIndex
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Logical interface number of VLAN."
    ::= { gamDhcpServerConfigVlanEntry 1 }

gamDhcpServerConfigVlanMode OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "VLAN mode of DHCP server. true is to enable DHCP server per VLAN and
         false is to disable it per VLAN."
    ::= { gamDhcpServerConfigVlanEntry 2 }

gamDhcpServerConfigExcludedTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF GAMDhcpServerConfigExcludedEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The table is DHCP server excluded IP onfiguration table. The indexes
         are low IP and high IP address."
    ::= { gamDhcpServerConfig 3 }

gamDhcpServerConfigExcludedEntry OBJECT-TYPE
    SYNTAX      GAMDhcpServerConfigExcludedEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry has a set of parameters"
    INDEX       { gamDhcpServerConfigExcludedLowIpAddress,
                  gamDhcpServerConfigExcludedHighIpAddress }
    ::= { gamDhcpServerConfigExcludedTable 1 }

GAMDhcpServerConfigExcludedEntry ::= SEQUENCE {
    gamDhcpServerConfigExcludedLowIpAddress   IpAddress,
    gamDhcpServerConfigExcludedHighIpAddress  IpAddress,
    gamDhcpServerConfigExcludedAction         GAMRowEditorState
}


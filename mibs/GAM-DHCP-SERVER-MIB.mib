    ::= { gamDhcpServerConfigExcludedEntry 2 }

gamDhcpServerConfigExcludedAction OBJECT-TYPE
    SYNTAX      GAMRowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION

-- *****************************************************************
-- DHCP-SNOOPING-MIB:  Positron's GAM private MIB
-- *****************************************************************

GAM-DHCP-SNOOPING-MIB DEFINITIONS ::= BEGIN

IMPORTS
    NOTIFICATION-GROUP, MODULE-COMPLIANCE, OBJECT-GROUP FROM SNMPv2-CONF
    NOTIFICATION-TYPE, MODULE-IDENTITY, OBJECT-TYPE FROM SNMPv2-SMI
    TEXTUAL-CONVENTION FROM SNMPv2-TC
    gamMgmt FROM POSITRON-SMI
    GAMInterfaceIndex FROM GAM-TC
    GAMVlan FROM GAM-TC
    GAMVlanOrZero FROM GAM-TC
    Integer32 FROM SNMPv2-SMI
    IpAddress FROM SNMPv2-SMI
    Unsigned32 FROM SNMPv2-SMI
    MacAddress FROM SNMPv2-TC
    TruthValue FROM SNMPv2-TC
    ;

gamDhcpSnoopingMib MODULE-IDENTITY
    LAST-UPDATED "201410100000Z"
    ORGANIZATION
        "Positron Access Solutions Corp."
    CONTACT-INFO
        "Customer Service <info@positronaccess.com>"
    DESCRIPTION
        "This is a private version of the DHCP Snooping MIB"
    REVISION    "201410100000Z"
    DESCRIPTION
        "Editorial changes"
    REVISION    "201407010000Z"
    DESCRIPTION
        "Initial version"
    ::= { gamMgmt 56 }


gamDhcpSnoopingMibObjects OBJECT IDENTIFIER
    ::= { gamDhcpSnoopingMib 1 }

gamDhcpSnoopingConfig OBJECT IDENTIFIER
    ::= { gamDhcpSnoopingMibObjects 2 }

gamDhcpSnoopingConfigGlobals OBJECT IDENTIFIER
    ::= { gamDhcpSnoopingConfig 1 }

gamDhcpSnoopingConfigGlobalsMode OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Indicates the DHCP snooping mode operation. Possible modes are - true:
         Enable DHCP snooping mode operation. When DHCP snooping mode operation
         is enabled, the DHCP request messages will be forwarded to trusted
         ports and only allow reply packets from trusted ports. false: Disable
         DHCP snooping mode operation."
    ::= { gamDhcpSnoopingConfigGlobals 1 }

gamDhcpSnoopingConfigInterfaceTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF GAMDhcpSnoopingConfigInterfaceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is a table of DHCP Snooping port configuration parameters"
    ::= { gamDhcpSnoopingConfig 2 }

gamDhcpSnoopingConfigInterfaceEntry OBJECT-TYPE
    SYNTAX      GAMDhcpSnoopingConfigInterfaceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each port has a set of parameters"
    INDEX       { gamDhcpSnoopingConfigInterfaceIfIndex }
    ::= { gamDhcpSnoopingConfigInterfaceTable 1 }

GAMDhcpSnoopingConfigInterfaceEntry ::= SEQUENCE {
    gamDhcpSnoopingConfigInterfaceIfIndex    GAMInterfaceIndex,
    gamDhcpSnoopingConfigInterfaceTrustMode  TruthValue
}

gamDhcpSnoopingConfigInterfaceIfIndex OBJECT-TYPE
    SYNTAX      GAMInterfaceIndex
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Logical interface number of the physical port."
    ::= { gamDhcpSnoopingConfigInterfaceEntry 1 }

gamDhcpSnoopingConfigInterfaceTrustMode OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Indicates the DHCP snooping port mode. Possible port modes are - true:
         Configures the port as trusted source of the DHCP messages. false:
         Configures the port as untrusted source of the DHCP messages."
    ::= { gamDhcpSnoopingConfigInterfaceEntry 2 }

gamDhcpSnoopingStatus OBJECT IDENTIFIER
    ::= { gamDhcpSnoopingMibObjects 3 }

gamDhcpSnoopingStatusAssignedIpTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF GAMDhcpSnoopingStatusAssignedIpEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is a table of assigned IP information in DHCP Snooping that are on
         single-tagged VLAN. This table is kept for compatibility."
    ::= { gamDhcpSnoopingStatus 1 }

gamDhcpSnoopingStatusAssignedIpEntry OBJECT-TYPE
    SYNTAX      GAMDhcpSnoopingStatusAssignedIpEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry has a set of parameters"
    INDEX       { gamDhcpSnoopingStatusAssignedIpMacAddress,
                  gamDhcpSnoopingStatusAssignedIpVlanId }
    ::= { gamDhcpSnoopingStatusAssignedIpTable 1 }

GAMDhcpSnoopingStatusAssignedIpEntry ::= SEQUENCE {
    gamDhcpSnoopingStatusAssignedIpMacAddress    MacAddress,
    gamDhcpSnoopingStatusAssignedIpVlanId        Integer32,
    gamDhcpSnoopingStatusAssignedIpIfIndex       GAMInterfaceIndex,
    gamDhcpSnoopingStatusAssignedIpIpAddress     IpAddress,
    gamDhcpSnoopingStatusAssignedIpNetmask       IpAddress,
    gamDhcpSnoopingStatusAssignedIpDhcpServerIp  IpAddress
}

gamDhcpSnoopingStatusAssignedIpMacAddress OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "MAC address."
    ::= { gamDhcpSnoopingStatusAssignedIpEntry 1 }

gamDhcpSnoopingStatusAssignedIpVlanId OBJECT-TYPE
    SYNTAX      Integer32 (1..4095)
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The VLAN id of the VLAN."

    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Netmask assigned to DHCP client by DHCP server."
    ::= { gamDhcpSnoopingStatusAssignedIp2TagsEntry 6 }

gamDhcpSnoopingStatusAssignedIp2TagsDhcpServerIp OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "IP address of the DHCP server that assigns the IP address and netmask."
    ::= { gamDhcpSnoopingStatusAssignedIp2TagsEntry 7 }


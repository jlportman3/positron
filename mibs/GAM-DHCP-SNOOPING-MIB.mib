    gamDhcpSnoopingStatusAssignedIp2TagsDhcpServerIp  IpAddress
}

gamDhcpSnoopingStatusAssignedIp2TagsMacAddress OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "MAC address."
    ::= { gamDhcpSnoopingStatusAssignedIp2TagsEntry 1 }

gamDhcpSnoopingStatusAssignedIp2TagsOuterVlanId OBJECT-TYPE
    SYNTAX      GAMVlan
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The outer VLAN id of the VLAN."
    ::= { gamDhcpSnoopingStatusAssignedIp2TagsEntry 2 }

gamDhcpSnoopingStatusAssignedIp2TagsInnerVlanId OBJECT-TYPE
    SYNTAX      GAMVlanOrZero
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The inner VLAN id of the VLAN. 0 means no inner VLAN ID (single tag)."
    ::= { gamDhcpSnoopingStatusAssignedIp2TagsEntry 3 }

gamDhcpSnoopingStatusAssignedIp2TagsIfIndex OBJECT-TYPE
    SYNTAX      GAMInterfaceIndex
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Logical interface number of the physical port of the DHCP client."

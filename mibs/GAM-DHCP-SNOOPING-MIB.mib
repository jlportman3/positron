GAMDhcpSnoopingStatusAssignedIp2TagsEntry ::= SEQUENCE {
    gamDhcpSnoopingStatusAssignedIp2TagsMacAddress    MacAddress,
    gamDhcpSnoopingStatusAssignedIp2TagsOuterVlanId   GAMVlan,
